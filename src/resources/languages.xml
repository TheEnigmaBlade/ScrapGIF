<Dictionary name="ScrapGIF" author="Enigma" prefix="giforg">

	<!-- General -->
	<record key="title">
		<value lang="en">ScrapGIF</value>
		<value lang="jp">スクラープGIF</value>
	</record>
	
	<!-- Menu bar -->
	<Dictionary prefix="menu">
		<!-- Library menu -->
		<Dictionary prefix="library">
			<record key="title">
				<value lang="en" mnemonic="l">Library</value>
			</record>
			<record key="new">
				<value lang="en" mnemonic="n">Create new...</value>
			</record>
			<record key="import">
				<value lang="en" mnemonic="i">Import...</value>
			</record>
			<record key="add">
				<value lang="en" mnemonic="a">Add image...</value>
			</record>
			<record key="addweb">
				<value lang="en" mnemonic="w">Add web image...</value>
			</record>
			<record key="addfolder">
				<value lang="en" mnemonic="o">Add from folder...</value>
			</record>
			<record key="manage">
				<value lang="en" mnemonic="m">Manage...</value>
			</record>
			<record key="export">
				<value lang="en" mnemonic="e">Export...</value>
			</record>
			<record key="exit">
				<value lang="en" mnemonic="x">Exit</value>
			</record>
		</Dictionary>
		<!-- View menu -->
		<Dictionary prefix="view">
			<record key="title">
				<value lang="en" mnemonic="v">View</value>
			</record>
			<record key="imagesize" mnemonic="s">
				<value lang="en">Image size</value>
			</record>
			<record key="showstarred" mnemonic="t">
				<value lang="en">Show starred only</value>
			</record>
			<record key="showuntagged" mnemonic="u">
				<value lang="en">Show untagged only</value>
			</record>
			
			<Dictionary prefix="imagesize">
				<record key="small" mnemonic="s">
					<value lang="en">Small</value>
				</record>
				<record key="normal" mnemonic="n">
					<value lang="en">Normal</value>
				</record>
				<record key="large" mnemonic="l">
					<value lang="en">Large</value>
				</record>
			</Dictionary>
		</Dictionary>
		<!-- Settings menu -->
		<Dictionary prefix="settings">
			<record key="title">
				<value lang="en" mnemonic="s">Settings</value>
			</record>
			<record key="checknewimages">
				<value lang="en" mnemonic="n">Monitor library folder for new images</value>
			</record>
			<record key="imagesize" mnemonic="s">
				<value lang="en">Image size</value>
			</record>
			<record key="uploadservice" mnemonic="u">
				<value lang="en">Upload service</value>
			</record>
			<record key="nativeframe" mnemonic="w">
				<value lang="en">Use system window (requires restart)</value>
			</record>
			<record key="language" mnemonic="l">
				<value lang="en">Language</value>
			</record>
		</Dictionary>
		<!-- About menu -->
		<Dictionary prefix="about">
			<record key="title">
				<value lang="en" mnemonic="a">About</value>
			</record>
			<record key="about">
				<value lang="en" mnemonic="a">About...</value>
			</record>
			<record key="changelog">
				<value lang="en" mnemonic="c">Changelog...</value>
			</record>
		</Dictionary>
	</Dictionary>
	
	<!-- Toolbar -->
	<Dictionary prefix="toolbar">
		<record key="search">
			<value lang="en">Search...</value>
		</record>
		<record key="favorites">
			<value lang="en">
				<text>Starred only</text>
				<tooltip>Only show starred images</tooltip>
			</value>
		</record>
	</Dictionary>
	
	<!-- Notifications -->
	<Dictionary prefix="notification">
		<Dictionary prefix="upload_error">
			<record key="none">
				<value lang="en">Image successfully uploaded</value>
			</record>
			<record key="no_service">
				<value lang="en">Upload failed: no supported service is available</value>
			</record>
			<record key="no_file">
				<value lang="en">Upload failed: the file wasn't found</value>
			</record>
			<record key="connection">
				<value lang="en">Upload failed: there was a problem with your internet</value>
			</record>
			<record key="unexpected">
				<value lang="en">Upload failed: an unexpected error occurred</value>
			</record>
			<record key="server">
				<value lang="en">Upload failed: the server returned an error</value>
			</record>
		</Dictionary>
		<Dictionary prefix="copy_error">
			<record key="none">
				<value lang="en">Image link copied</value>
			</record>
			<record key="no_service">
				<value lang="en">Copy failed: no supported service is available</value>
			</record>
		</Dictionary>
	</Dictionary>
	
	<!-- Dialogs -->
	<Dictionary prefix="dialog">
		<Dictionary prefix="about">
			<record key="title">
				<value lang="en">About ScrapGIF</value>
			</record>
			
		</Dictionary>
	</Dictionary>
</Dictionary>